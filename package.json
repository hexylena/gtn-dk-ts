{
  "name": "ptdk",
  "version": "1.1.0",
  "description": "GTN Training Development Kit",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "build": "npm run build-web; npm run build-cli",
    "build-web": "esbuild --outdir=dist/ src/index.ts --sourcemap --minify --platform=browser --bundle",
    "build-cli": "esbuild --outdir=dist/ src/cli.ts --minify --platform=node --bundle",
    "check": "tsc",
    "fmt": "prettier -w src/*.ts",
    "ex": "node ./dist/cli.js --wf ex/Galaxy-Workflow-mRNA-Seq_BY-COVID_Pipeline__Analysis.ga"
  },
  "author": "Helena, Bérénice",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.29",
    "esbuild": "^0.20.2",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "typescript": "^5.4.2",
    "undici-types": "^6.9.0"
  }
}
